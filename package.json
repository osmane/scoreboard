{
  "name": "scoreboard",
  "version": "1.0.0",
  "description": "scoreboard for tailuge/billiards running on cyclic.sh",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "concurrently \"npx tsc -w\" \"node --watch dist/index.js\"",
    "start": "node dist/index.js",
    "prettify": "prettier --cache true --write --trailing-comma es5 --no-semi  src/*.ts src/**/*.ts dist/**/*.{css,html} *.json",
    "deps": "npx yarn-check --skip-unused",
    "test": "jest"
  },
  "keywords": [],
  "author": "tailuge",
  "license": "ISC",
  "dependencies": {
    "@cyclic.sh/dynamodb": "^0.0.35",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsoncrush": "^1.1.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.7",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.16",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
